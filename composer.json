{
    "name": "cgdsoftware/tables",
    "description": "Data Table library with server-side processing and a VueJS component",
    "keywords": [
        "cgdsoftware",
        "datatable",
        "data-table",
        "data-table-server-side"
    ],
    "homepage": "https://github.com/cgdsoftware/tables",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Ocneanu",
            "email": "aocneanu@gmail.com",
            "homepage": "https://cgdsoftware.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.0",
        "box/spout": "^3.0",
        "laravel/framework": "^9.0",
        "cgdsoftware/enums": "^2.0",
        "cgdsoftware/filters": "^2.0",
        "cgdsoftware/helpers": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelEnso\\Tables\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelEnso\\Tables\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelEnso\\Tables\\AppServiceProvider"
            ],
            "aliases": {}
        }
    }
}
